                    "row_nums": [
                        6
                    ],
                    "column_header": false,
                    "subcell": false,
                    "projected_row_header": false,
                    "cell_text": "Scotland (UK)"
                },
                {
                    "bbox": [
                        0.2593621261981355,
                        0.6094161008712515,
                        0.8059949345949318,
                        0.6431057674018335
                    ],
                    "column_nums": [
                        1
                    ],
                    "row_nums": [
                        6
                    ],
                    "column_header": false,
                    "subcell": false,
                    "projected_row_header": false,
                    "cell_text": "The Scottish Executive Education Department (SEED) has responsibility for pre-\nschool provision."
                },
                {
